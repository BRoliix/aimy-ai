# Agentic AI Assistant Configuration

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Speech Configuration
SPEECH_RECOGNITION_TIMEOUT=5
SPEECH_RECOGNITION_PHRASE_TIMEOUT=1
TTS_RATE=200
TTS_VOLUME=0.9

# Wake Word Configuration
WAKE_WORD=assistant

# System Configuration
ENABLE_SYSTEM_COMMANDS=true
ENABLE_WEB_COMMANDS=true
ENABLE_FILE_OPERATIONS=true

# Security Settings
REQUIRE_CONFIRMATION_FOR_SYSTEM_CHANGES=true
ALLOWED_APPLICATIONS=safari,chrome,firefox,vscode,finder,terminal,calculator,notes
BLOCKED_COMMANDS=rm,delete,format,shutdown,reboot